{
  "name": "knuts-and-bolts",
  "version": "1.0.0",
  "description": "data structures and algorithms implemented in JavaScript",
  "main": "y",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "coverage": "open ./coverage/index.html",
    "test": "jest --coverage --colors",
    "type-check": "tsc --noEmit"
  },
  "presets": [
    "@babel/env"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "html"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "enableTsDiagnostics": true
      }
    },
    "testRegex": "\\.test\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Jessica-Koch/knuts-and-bolts.git"
  },
  "author": "Jessica Koch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jessica-Koch/knuts-and-bolts/issues"
  },
  "homepage": "https://github.com/Jessica-Koch/knuts-and-bolts#readme",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {}
}
